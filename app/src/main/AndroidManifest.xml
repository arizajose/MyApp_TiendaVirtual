<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapp_tiendavirtual">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".beans.ClaseGlobal"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp_TiendaVirtual">
        <activity android:name=".BoletaActivity"></activity>
        <activity android:name=".CompraActivity" />
        <activity android:name=".GlobalCarritoActivity" />
        <activity android:name=".ArticuloActivity" />
        <activity android:name=".CategoriaActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>